-------------------------------------------------------------------------------
module Test {
  fun a : String {
    "Hello"
  }

  fun b : Function(String) {
    Test.a
  }
}

component Main {
  fun render : String {
    let x =
      Test.b()

    x()
  }
}
-------------------------------------------------------------------------------
store Test {
  get a : String {
    "Hello"
  }

  fun b : String {
    Test.a
  }
}
-------------------------------------------------------------------------------
record Test {
  test : String
}

provider Test : Test {
  fun print (a : String) : String {
    a
  }
}

component Main {
  use Test {
    test: ""
  }

  fun render : String {
    Test.print("x")
  }
}
-------------------------------------------------------------------------------
record Test {
  test : String
}

provider Test : Test {
  fun print (a : String) : String {
    a
  }
}

component Main {
  use Test {
    test: ""
  }

  fun render : String {
    Test.subscriptions
    Test.print("a")
  }
}
