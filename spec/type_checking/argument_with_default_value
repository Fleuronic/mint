component Main {
  fun test (a : String = "Hello") : String {
    a
  }

  fun render : String {
    test()
  }
}
-------------------------------------------FunctionArgumentMustHaveADefaultValue
component Main {
  fun test (a : String = "Hello", b : String) : Number {
    a
  }

  fun render : String {
    test()
  }
}
