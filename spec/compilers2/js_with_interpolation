component Main {
  fun test : String {
    "World!"
  }

  fun render : String {
    `      "Hello" + #{test()}   `
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  a() {
    return `World!`;
  }

  render() {
    return ("Hello" + this.a());
  }
};

A.displayName = "Main";
